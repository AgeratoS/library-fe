{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@/components": ["components"],
            "@/components/*": ["components/*"],
            "@/modules": ["modules"],
            "@/modules/*": ["modules/*"],
            "@/utils": ["utils"],
            "@/utils/*": ["utils/*"],
            "@/appTypes": ["types"],
            "@/appTypes/*": ["types/*"],
            "@/constants": ["constants"],
            "@/constants/*": ["constants/*"]
        }
    }
}